        "symfony/css-selector": "~3.0",
        "symfony/dom-crawler": "~3.0",
        "monolog/monolog": "~1.0",
        "desarrolla2/cache":  "~2.0",
        "maximebf/debugbar": "1.*",
        "erusev/parsedown": "~1.6",
        "nesbot/carbon": "~1.21",
        "swiftmailer/swiftmailer": "~5.4",
        "guzzlehttp/guzzle": "~6.0",
